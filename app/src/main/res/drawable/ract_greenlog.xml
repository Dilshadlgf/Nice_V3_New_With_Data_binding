<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="@dimen/_40sdp"
                android:height="@dimen/_40sdp" />
            <solid android:color="#2C5744" />
            <corners
                     android:radius="@dimen/_10sdp" />
        </shape>
    </item>

    <item android:top="@dimen/_50sdp">
        <shape android:shape="rectangle">
            <size
                android:width="@dimen/_30sdp"
                android:height="@dimen/_30sdp" />
            <solid android:color="#9cc634" />
            <corners
                android:bottomLeftRadius="@dimen/_10sdp"
                android:bottomRightRadius="@dimen/_10sdp"/>
        </shape>

    </item>

</layer-list>